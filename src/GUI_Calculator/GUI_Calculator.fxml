<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI_Calculator.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="350.0" prefWidth="400.0">
         <children>
            <TextArea fx:id="Results" editable="false" layoutX="151.0" layoutY="134.0" prefHeight="12.0" prefWidth="99.0" />
            <Button fx:id="Result" layoutX="174.0" layoutY="88.0" mnemonicParsing="false" onAction="#returnResult" text="Result" />
            <TextField fx:id="EnterOperation" layoutX="35.0" layoutY="29.0" prefHeight="38.0" prefWidth="331.0" promptText="EnterOperation" />
            <Button fx:id="showOperations" layoutX="141.0" layoutY="186.0" mnemonicParsing="false" onAction="#showOperations" text="See my operations" />
            <TextArea fx:id="myOperations" editable="false" layoutX="31.0" layoutY="236.0" prefHeight="100.0" prefWidth="331.0" />
         </children>
      </Pane>
   </children>
</GridPane>
