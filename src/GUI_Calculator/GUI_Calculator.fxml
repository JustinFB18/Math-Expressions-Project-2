<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI_Calculator.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="430.0" prefWidth="400.0">
         <children>
            <TextArea fx:id="Results" editable="false" layoutX="149.0" layoutY="196.0" prefHeight="12.0" prefWidth="99.0" />
            <Button fx:id="Result" layoutX="174.0" layoutY="142.0" mnemonicParsing="false" onAction="#returnResult" text="Result" />
            <TextField fx:id="EnterOperation" layoutX="33.0" layoutY="79.0" prefHeight="38.0" prefWidth="331.0" promptText="EnterOperation" />
            <Button fx:id="showOperations" layoutX="141.0" layoutY="253.0" mnemonicParsing="false" onAction="#showOperations" text="See my operations" />
            <TextArea fx:id="myOperations" editable="false" layoutX="33.0" layoutY="300.0" prefHeight="100.0" prefWidth="331.0" />
            <TextField fx:id="userName" layoutX="132.0" layoutY="34.0" promptText="Write your username" />
            <Label layoutX="62.0" layoutY="34.0" text="Username" />
         </children>
      </Pane>
   </children>
</GridPane>
